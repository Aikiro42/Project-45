{
  "globalTagDefaults" : {
    "paletteSwaps" : "",
    "chargeFrame": "1",
    "directives": ""
  },

  "animatedParts" : {
    "stateTypes" : {


      // compatibility
      "firing" : {
        "default" : "off",
        "states" : {
          "off" : {},
          "charge" : {},
          "fire" : {
            "frames" : 2,
            "cycle" : 0.07,
            "mode" : "transition",
            "transition" : "off"
          }
        }
      },

      "altfiring": {
        "default" : "off",
        "states" : {
          "off" : {},
          "charge" : {},
          "fire" : {
            "frames" : 2,
            "cycle" : 0.07,
            "mode" : "transition",
            "transition" : "off"
          }
        }
      },

      /*
      "erchiuseye" : {
        "default" : "idle",
        "states" : {
          "idle" : {},
          "squirm" : {},
          "fire" : {}
        }
      },
      */

      "gun" : {
        "default" : "idle",
        "states" : {
          "idle": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },

          // for break action guns
          // (This animation state can only be accessed if the <breakAction> config in the primaryAbility
          // is true, and will work given you've configured the middle's frames to include a set
          // of "open.<frame>" frames)
          "open": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },

          // firing -> idle
          // (FORCED) firing -> ejecting
          // (You can set the transition to "fired" in the weapon's custom animation config,
          // given you've configured the frames to include a "fired.<frame>" set)
          "firing": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "idle"
          },
          "fired": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },
          "firingLoop": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },

          // ejecting -> ejected
          // (FORCED) ejected -> feeding
          "ejecting": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "ejected"
          },
          "boltPulling": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "ejected"
          },
          "ejected": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },

          // feeding -> idle
          // (FORCED) feeding -> jammed
          "feeding": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "idle"
          },
          "boltPushing": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "idle"
          },

          // unjamming -> jammed
          // (FORCED) unjamming -> feeding
          "jammed": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "loop"
          },
          "unjamming": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition": "jammed"
          }

        }
      },
      
      "flash" : {
        "default": "off",
        "states":{
          "off" : {},
          "flash" : {
            "frames" : 2,
            "cycle" : 0.07,
            "mode" : "transition",
            "transition" : "off"
          }
        }
      },

      "altflash" : {
        "default": "off",
        "states":{
          "off" : {},
          "flash" : {
            "frames" : 2,
            "cycle" : 0.07,
            "mode" : "transition",
            "transition" : "off"
          }
        }
      },
      
      "charge": {
        "default": "off",
        "states": {
          "off": {},
          "chargingProg": {},
          "charging": {
            "frames" : 1,
            "cycle" : 0.25,
            "mode" : "transition",
            "transition" : "off"
          }
        }
      },
      "chamber": {
        "default": "ready",
        "states": {
          "empty": {},
          "ready": {},
          "filled": {}
        }
      },
      "bolt": {
        "default": "closed",
        "states": {
          "open": {},
          "closed": {},
          "jammed": {}
        }
      },
      "magazine": {
        "default": "present",
        "states": {
          "present": {},
          "absent": {}
        }
      }
    },

    "parts" : {
